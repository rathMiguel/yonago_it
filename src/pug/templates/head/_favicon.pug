if favicon.enable == true
	meta(name='mobile-web-app-capable', content='yes')
	meta(name='theme-color', content=favicon.themeColor)
	link(rel='manifest', href=label.depth + 'favicons/manifest.json')
	if favicon.icons.android
		link(rel='icon', type='image/png', sizes='192x192', href=label.depth + 'favicons/android-chrome-192x192.png')
	if favicon.icons.appleIcon
		link(rel='apple-touch-icon', sizes='57x57', href=label.depth + 'favicons/apple-touch-icon-57x57.png')
		link(rel='apple-touch-icon', sizes='60x60', href=label.depth + 'favicons/apple-touch-icon-60x60.png')
		link(rel='apple-touch-icon', sizes='72x72', href=label.depth + 'favicons/apple-touch-icon-72x72.png')
		link(rel='apple-touch-icon', sizes='76x76', href=label.depth + 'favicons/apple-touch-icon-76x76.png')
		link(rel='apple-touch-icon', sizes='114x114', href=label.depth + 'favicons/apple-touch-icon-114x114.png')
		link(rel='apple-touch-icon', sizes='120x120', href=label.depth + 'favicons/apple-touch-icon-120x120.png')
		link(rel='apple-touch-icon', sizes='144x144', href=label.depth + 'favicons/apple-touch-icon-144x144.png')
		link(rel='apple-touch-icon', sizes='152x152', href=label.depth + 'favicons/apple-touch-icon-152x152.png')
		link(rel='apple-touch-icon', sizes='180x180', href=label.depth + 'favicons/apple-touch-icon-180x180.png')
		meta(name='apple-mobile-web-app-capable', content='yes')
	if favicon.icons.favicons
		link(rel='icon', type='image/png', sizes='16x16', href=label.depth + 'favicons/favicon-16x16.png')
		link(rel='icon', type='image/png', sizes='230x230', href=label.depth + 'favicons/favicon-230x230.png')
		link(rel='icon', type='image/png', sizes='32x32', href=label.depth + 'favicons/favicon-32x32.png')
		link(rel='icon', type='image/png', sizes='96x96', href=label.depth + 'favicons/favicon-96x96.png')
		link(rel='shortcut icon', href=label.depth + 'favicons/favicon.ico')
	if favicon.icons.windows
		meta(name='msapplication-TileColor', content=favicon.themeColor)
		meta(name='msapplication-TileImage', content=label.depth + 'favicons/mstile-144x144.png')
		meta(name='msapplication-config', content=label.depth + 'favicons/browserconfig.xml')
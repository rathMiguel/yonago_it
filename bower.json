{
  "name": "neue-component",
  "description": "this component is builld up for Website",
  "main": "gulpfile.js",
  "authors": [
    "kenji Hashimoto"
  ],
  "license": "ISC",
  "keywords": [
    "neue"
  ],
  "moduleType": [],
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "dev",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "^3.1.1",
    "reset-css": "^2.2.1",
    "font-awesome": "^5.0.13",
    "slick-carousel": "^1.8.1",
    "matchHeight": "^0.7.2",
    "mixitup": "https://github.com/patrickkunka/mixitup/",
    "instafeed.js": "^1.4.1"
  },
  "overrides": {
    "reset-css": {
      "main": "reset.css"
    },
    "matchHeight": {
      "dependencies": {
        "jquery": ">=1.7"
      }
    },
    "slick-carousel": {
      "main": [
        "slick/slick.min.js",
        "slick/slick.css",
        "slick/slick-theme.css"
      ]
    },
    "font-awesome": {
      "main": [
        "web-fonts-with-css/scss/fa-brands.scss",
        "web-fonts-with-css/scss/fa-regular.scss",
        "web-fonts-with-css/scss/fa-solid.scss",
        "web-fonts-with-css/scss/fontawesome.scss",
        "web-fonts-with-css/webfonts/**/*"
      ]
    },
    "mixitup": {
      "main": [
        "dist/mixitup.min.js"
      ]
    }
  }
}
